                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select prod_codigo,prod_nombre,
(select  top 1 prbr_codcsg 
	from dba.prodbienraiz as pre
	where prod.prod_codigo=pre.prod_codigo and pre.prbr_estado=1 and prbr_codcsg  is not null ) as csg
,
'chile' as Pais,
prov.prov_nombre as Provincia,
r.regi_nomcor as Region,
prod_direcc
 from dba.productores as prod inner join dba.comunasexp as com on
 prod.comu_codigo=com.comu_codigo inner join dba.provincias as prov on
 com.prov_codigo=prov.prov_codigo and
 com.regi_codigo=prov.regi_codigo join dba.regiones as r on
 com.regi_codigo=r.regi_codigo
where prod_codigo in(
select distinct p.prod_codigo 
from dba.prodbienraiz as p inner join dba.prodcuarteles as c on
p.prod_codigo=c.prod_codigo and
p.prbr_codpre=c.prbr_codpre 
