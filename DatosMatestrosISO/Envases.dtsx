            <component
              refId="Package\Envases\envases"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="envases"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset">[dba].[categorias]</property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select  distinct 
	   [cod_envase] = emb.emba_codigo
     ,[nombre] = emb.emba_codigo + ' - ' + eti.etiq_nombre + ' - ' + 
		              CASE WHEN env.enva_pesone &gt;= 10 THEN LEFT(CONVERT(VARCHAR(5),env.enva_pesone),4) 
					     ELSE LEFT(CONVERT(VARCHAR(5),env.enva_pesone),3) 
				     END + ' KN.'
		
      ,[peso_neto] = env.enva_pesone
      ,[peso_bruto] = env.enva_pesobr
      ,[peso_declarado] = env.enva_pesone
      ,Gtin = (select top 1 gt.gtin_codigo
	  from dba.spro_gtin as gt 
	  where  emb.emba_abrcod=gt.emba_abrcod and
			  env.enva_pesone=gt.enva_pesone and
			  esp.espe_codigo=gt.espe_codigo and
			  env.enva_tipoen=gt.enva_tipoen
			   and gt.gtin_estado=1 ) ,
Material=emb.emba_abrevi
  from dba.embalajes emb join dba.envases env on
  emb.enva_codigo = env.enva_codigo 
  and emb.enva_tipoen = env.enva_tipoen
  join dba.envases_especies esp on
  esp.enva_codigo = env.enva_codigo 
  and esp.enva_tipoen = env.enva_tipoen  inner join dba.embalajes_abreviados as Ema on
  emb.emba_abrcod=Ema.emba_abrcod inner join   dba.etiquetas AS eti on
  emb.etiq_codigo = eti.etiq_codigo
  where  esp.espe_codigo =?   and env.enva_estado = 1  and emb. emba_estado = 1
  
union all
SELECT    cod_envase =Convert(varchar(20), FORMAT(env.enva_tipoen,'0') + FORMAT(env.enva_codigo,'000')),
        nombre = env.enva_nombre,
        peso_neto = env.enva_pesone, 
        peso_bruto = env.enva_pesobr,
        peso_declarado = env.enva_pesone,
        gtin = '',
        material = env.enva_nombre
FROM    dba.envases_especies AS esp,
        dba.envases AS env,
        dba.tiposenvases AS ten
WHERE    esp.enva_tipoen = env.enva_tipoen
AND        esp.enva_codigo = env.enva_codigo
AND        env.enva_tipoen = ten.enva_tipoen
AND        esp.espe_codigo = ?
AND        ten.tien_usoenv = 1
AND        env.enva_ctlcon = 1
and         env.enva_estado = 1</property>
